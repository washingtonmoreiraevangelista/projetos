{
  "editor.tabSize": 2,
  "editor.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
  "editor.fontWeight": "500",
  "editor.lineHeight": 20,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "boundary",
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.defaultProfile.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "vscode_custom_css.imports": [
    "file:///C:/Users/mujog/vscode.css"
  ],
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**"
  ],
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "debug.javascript.pickAndAttachOptions": {
    "attachExistingChildren": false
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "javascript.format.semicolons": "ignore",
  "javascript.referencesCodeLens.enabled": true,
  "typescript.format.semicolons": "remove",
  "terminal.integrated.tabs.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "vs-kubernetes": {
    "vscode-kubernetes.kubectl-path.linux": "/home/mujo/.local/state/vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.helm-path.linux": "/home/mujo/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.minikube-path.linux": "/home/mujo/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.crd-code-completion": "enabled",
    "vscode-kubernetes.helm-path.windows": "C:\\Users\\mujog\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
    "vscode-kubernetes.minikube-path.windows": "C:\\Users\\mujog\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe",
    "vs-kubernetes.kubeconfig": "/home/mujo/.kube/config",
    "vs-kubernetes.knownKubeconfigs": [
      "/home/mujo/.kube/config"
    ]
  },
  "terminal.external.linuxExec": "bash",
  "scm.repositories.visible": 50,
  "files.associations": {
    ".env": "dotenv"
  },
  "scm.alwaysShowActions": true,
  "scm.providerCountBadge": "visible",
  "scm.defaultViewMode": "tree",
  "debug.terminal.clearBeforeReusing": true,
  "redhat.telemetry.enabled": false,
  "debug.javascript.unmapMissingSources": true,
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "vscode_custom_css.statusbar": true,
  "material-icon-theme.activeIconPack": "nest",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "git.ignoreRebaseWarning": true,
  "files.autoSave": "afterDelay",
  "vscode-kubernetes.log-viewer.follow": false,
  "vscode-kubernetes.log-viewer.timestamp": true,
  "vscode-kubernetes.log-viewer.since": -1,
  "vscode-kubernetes.log-viewer.tail": -1,
  "vscode-kubernetes.log-viewer.destination": "Webview",
  "vscode-kubernetes.log-viewer.wrap": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.codeLens": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "terminal.integrated.tabs.location": "left",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.gpuAcceleration": "on",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "terminal.integrated.env.linux": {},
  "remote.autoForwardPortsSource": "hybrid",
  "editor.rulers": [
    120
  ],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#555555",
  },
  "editor.minimap.showSlider": "always",
  "workbench.panel.showLabels": false
}